    "# 2023-03-07 Codewars\n",
    "# Narcissistic Number\n",
    "number = 167\n",
    "def checkNarcissistic(number):\n",
    "    powerList = [int(digit)**len(str(number)) for digit in str(number)]\n",
    "    \n",
    "    return sum(powerList)== number"
   ]
